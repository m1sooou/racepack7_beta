textures/photorealistic_marble/marble01
{
	qer_editorimage textures/photorealistic_marble/marble01.tga

	{
		map textures/photorealistic_marble/marble01.tga
	        rgbGen identity
		tcGen environment
		tcmod scale .75 .75
	}
	
	{
		map textures/photorealistic_marble/marble01.tga
		blendFunc GL_ONE_MINUS_SRC_ALPHA GL_SRC_ALPHA	
		rgbGen identity
	}
	{
		map $lightmap
		blendfunc gl_dst_color gl_zero
		rgbGen identity
	}
}

